
pipeline{
    agent any
    stages {
        stage ('Build'){
            steps{
                echo " build pipeline..!"
            }
        }
        stage('scan'){
            steps{
                echo "scan pipeline"
            }
        }
        stage('Build docker'){
            steps{
                echo "build docker pipeline"
            }

        }
        stage('devploye'){
            steps{
                echo "dev pipeline"
            }
        }
    }
   
}
